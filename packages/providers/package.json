{
  "name": "@agentic/providers",
  "version": "0.1.0",
  "description": "LLM and agent provider implementations for @agentic/triage",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ollama": {
      "types": "./dist/ollama.d.ts",
      "import": "./dist/ollama.js"
    },
    "./jules": {
      "types": "./dist/jules.d.ts",
      "import": "./dist/jules.js"
    },
    "./cursor": {
      "types": "./dist/cursor.d.ts",
      "import": "./dist/cursor.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "peerDependencies": {
    "@agentic/triage": ">=0.2.0"
  },
  "devDependencies": {
    "@agentic/triage": "github:agentic-dev-library/triage#1a507f900b4aae96eea51f85e6c68ba4b72b54a8",
    "tsup": "^8.5.0",
    "typescript": "^5.3.0"
  }
}
