#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/@agentic-dev-library+triage@1.1.0_hono@4.11.1/node_modules/@agentic-dev-library/triage/dist/node_modules:/workspace/node_modules/.pnpm/@agentic-dev-library+triage@1.1.0_hono@4.11.1/node_modules/@agentic-dev-library/triage/node_modules:/workspace/node_modules/.pnpm/@agentic-dev-library+triage@1.1.0_hono@4.11.1/node_modules/@agentic-dev-library/node_modules:/workspace/node_modules/.pnpm/@agentic-dev-library+triage@1.1.0_hono@4.11.1/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/@agentic-dev-library+triage@1.1.0_hono@4.11.1/node_modules/@agentic-dev-library/triage/dist/node_modules:/workspace/node_modules/.pnpm/@agentic-dev-library+triage@1.1.0_hono@4.11.1/node_modules/@agentic-dev-library/triage/node_modules:/workspace/node_modules/.pnpm/@agentic-dev-library+triage@1.1.0_hono@4.11.1/node_modules/@agentic-dev-library/node_modules:/workspace/node_modules/.pnpm/@agentic-dev-library+triage@1.1.0_hono@4.11.1/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@agentic-dev-library/triage/dist/cli.js" "$@"
else
  exec node  "$basedir/../@agentic-dev-library/triage/dist/cli.js" "$@"
fi
