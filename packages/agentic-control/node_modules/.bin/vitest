#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/vitest@4.0.16_@opentelemetry+api@1.9.0_@types+node@22.19.3_tsx@4.21.0_yaml@2.8.2/node_modules/vitest/node_modules:/workspace/node_modules/.pnpm/vitest@4.0.16_@opentelemetry+api@1.9.0_@types+node@22.19.3_tsx@4.21.0_yaml@2.8.2/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/vitest@4.0.16_@opentelemetry+api@1.9.0_@types+node@22.19.3_tsx@4.21.0_yaml@2.8.2/node_modules/vitest/node_modules:/workspace/node_modules/.pnpm/vitest@4.0.16_@opentelemetry+api@1.9.0_@types+node@22.19.3_tsx@4.21.0_yaml@2.8.2/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
