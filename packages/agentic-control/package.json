{
  "name": "agentic-control",
  "version": "1.1.0",
  "description": "Unified AI agent fleet management, triage, and orchestration toolkit for control centers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agentic": "./dist/cli.js",
    "agentic-control": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fleet": {
      "types": "./dist/fleet/index.d.ts",
      "import": "./dist/fleet/index.js"
    },
    "./triage": {
      "types": "./dist/triage/index.d.ts",
      "import": "./dist/triage/index.js"
    },
    "./github": {
      "types": "./dist/github/index.d.ts",
      "import": "./dist/github/index.js"
    },
    "./handoff": {
      "types": "./dist/handoff/index.d.ts",
      "import": "./dist/handoff/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./sandbox": {
      "types": "./dist/sandbox/index.d.ts",
      "import": "./dist/sandbox/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev:cli": "tsx src/cli.ts",
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "biome lint src/ tests/",
    "lint:fix": "biome lint --write src/ tests/",
    "format": "biome format --write src/ tests/",
    "format:check": "biome format src/ tests/",
    "check": "biome check src/ tests/",
    "check:fix": "biome check --write src/ tests/",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "dependencies": {
    "@ai-sdk/mcp": "^0.0.12",
    "@inquirer/prompts": "8.1.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@octokit/rest": "^22.0.1",
    "ai": "^5.0.113",
    "commander": "^14.0.2",
    "cosmiconfig": "9.0.0",
    "env-var": "^7.5.0",
    "simple-git": "^3.30.0",
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^2.0.53",
    "@ai-sdk/azure": "^2.0.78",
    "@ai-sdk/google": "^2.0.44",
    "@ai-sdk/mistral": "^2.0.25",
    "@ai-sdk/openai": "^2.0.76",
    "ai-sdk-ollama": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/mistral": {
      "optional": true
    },
    "@ai-sdk/azure": {
      "optional": true
    },
    "ai-sdk-ollama": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.53",
    "@types/node": "^22.15.0",
    "tsx": "4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.14",
    "vitest-agentic-control": "workspace:*"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "ai",
    "agent",
    "fleet",
    "orchestration",
    "triage",
    "sandbox",
    "docker",
    "cursor",
    "anthropic",
    "openai",
    "claude",
    "ollama",
    "github",
    "automation",
    "devops",
    "typescript",
    "cli",
    "mcp",
    "property-based-testing"
  ],
  "author": "Jon Bogaty",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbdevprimary/agentic-control.git",
    "directory": "packages/agentic-control"
  },
  "homepage": "https://github.com/jbdevprimary/agentic-control#readme",
  "bugs": {
    "url": "https://github.com/jbdevprimary/agentic-control/issues"
  }
}
